{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@Api": ["api"],
      "@Constants": ["constants"],
      "@Elements": ["components/Elements"],
      "@Elements/*": ["components/Elements/*"],
      "@Form": ["components/Form"],
      "@Form/*": ["components/Form/*"],
      "@Layout": ["components/Layout"],
      "@Layout/*": ["components/Layout/*"],

      "@Helpers/*": ["helpers/*"],
      "@Helpers": ["helpers"],
      "@Lib/*": ["lib/*"],
      "@Lib": ["lib"],
      "@Pages/*": ["pages/*"],
      "@Pages": ["pages"],
      "@Providers/*": ["providers/*"],
      "@Providers": ["providers"],
      "@Routes/*": ["routes/*"],
      "@Routes": ["routes"],
      "@Services/*": ["services/*"],
      "@Services": ["services"],
      "@Store/*": ["store/*"],
      "@Store": ["store"],
      "@Types/*": ["types/*"],
      "@Types": ["types"],
      "@Utils/*": ["utils/*"],
      "@Utils": ["utils"],
      "@Common/*": ["../../common/*"],
      "@Common": ["../../common"]
    }
  }
}
